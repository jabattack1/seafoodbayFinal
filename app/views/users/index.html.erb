<div class="iform" id="account" style="overflow-y: auto">
<form method="POST" action="/users">

	<p class="iheader">Create User Account</p>

	<label class="ilabel">
		<input type="text" name="first_name" placeholder="First Name"class="itext"/>
	</label><br>

	<label class="ilabel">
		<input type="text" name="last_name" placeholder="Last Name" class="itext"/>
	</label><br>

	<label class="ilabel">
		<input type="text" name="phone" placeholder="Phone" class="itext"/>
	</label><br>

	<label class="ilabel">
		<input type="text" name="email" placeholder="Email" class="itext"/>
	</label><br>

	<label class="ilabel">
		<input type="text" name="address" placeholder="Address (street, City, State, Zip)" class="itext"/>
	</label><br>

	<label class="ilabel">
		<input type="text" name="password" placeholder="Password(at least 5 characters)"class="itext"/><br>
		<input name="authenticity_token" value="<%= form_authenticity_token %>" type="hidden">
	</label>

	<input type="submit" id="account_submit" class="ibutton"/>

</form>
</div>



	